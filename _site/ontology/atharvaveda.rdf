<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#paippaladaHasGenre">
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/indology#vedaHasGenre"/>
    <rdfs:comment xml:lang="en">Relating a Paippalada to its genre.</rdfs:comment>
    <rdfs:label xml:lang="de">Paippalada hat Gattung</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/indology#Samhita"/>
    <rdfs:label xml:lang="en">Paippalada has genre</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isNormalizationOf">
    <rdfs:comment xml:lang="en">Relating a normalized Padapatha to its Samhitapatha.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Normalisierung von</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/indology#Samhitapatha"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Padapatha"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#isNormalizationOf"/>
    <rdfs:label xml:lang="en">is normalization of</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isVerseOfHalfStrophe">
    <rdfs:label xml:lang="de">ist Vers einer Halbstrophe</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">is verse of half-strophe</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda verse to a half-strophe it is part of.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#HalfStrophe"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Verse"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#isVerseOfHalfStrophe"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isStropheOfSukta">
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#isStropheOfPoem"/>
    <rdfs:label xml:lang="de">ist Strophe einer Sukta</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Sukta"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Strophe"/>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda strophe to its sukta.</rdfs:comment>
    <rdfs:label xml:lang="en">is strophe of sukta</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isVerseOfStrophe">
    <rdfs:label xml:lang="de">ist Vers einer Strophe</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Strophe"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#isVerseOfStrophe"/>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda verse to a strophe it is part of.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Verse"/>
    <rdfs:label xml:lang="en">is verse of strophe</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isKandaOfAtharvaveda">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="de">ist Kanda einer Atharvaveda</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Atharvaveda"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Kanda"/>
    <rdfs:label xml:lang="en">is kanda of Atharvaveda</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a kanda to its Atharvaveda.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#isSuktaOfKanda">
    <rdfs:label xml:lang="en">is sukta of kanda</rdfs:label>
    <rdfs:label xml:lang="de">ist Sukta einer Kanda</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Kanda"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Sukta"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda sukta to its kanda.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/literature#isVerseSongOfCollection"/>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/atharvaveda">
    <dct:contributor xml:lang="en">Magdalena Plamada, PhD, Zürich University, Switzerland</dct:contributor>
    <dct:title xml:lang="en">An ontology about Atharvaveda</dct:title>
    <dct:description xml:lang="en">Formal description of specific concepts in the scientific study of Atharvaveda.</dct:description>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:contributor xml:lang="en">Reto Baumgartner, tool coordinator and software developer, University of Zürich, Switzerland</dct:contributor>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#paippaladaHasRecitingStyle">
    <rdfs:label xml:lang="de">Paippalada hat Rezitationsstil</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a Paippalada to a reciting style it has.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">Paippalada has reciting style</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/indology#vedaHasRecitingStyle"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/indology#Samhitapatha"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Sukta">
    <rdfs:comment xml:lang="en">Sukta in the Atharvaveda.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/literature#VerseSong"/>
    <rdfs:label xml:lang="sa">Atharvaveda sūkta</rdfs:label>
    <rdfs:label xml:lang="de">Atharvaveda-Sukta</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/indology#Expression"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">Atharvaveda sukta</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text#HandwrittenText"/>
    <skos:note xml:lang="en">The Atharvaveda contains 730 Suktas.</skos:note>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Padapatha">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text#Normalization"/>
    <rdfs:comment xml:lang="en">Padapatha normalized from a Samhitapatha.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/indology#Padapatha"/>
    <rdfs:label xml:lang="de">Normalisierte Padapatha</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">Normalized Padapatha</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#HalfStrophe">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#HalfStrophe"/>
    <rdfs:comment xml:lang="en">Half a strophe in the Atharvaveda.</rdfs:comment>
    <rdfs:label xml:lang="en">Atharvaveda half-strophe</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="de">Atharvaveda-Halbstrophe</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Verse">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#Verse"/>
    <rdfs:comment xml:lang="en">Verse as part of a strophe in the Atharvaveda.</rdfs:comment>
    <skos:note xml:lang="en">The Atharvaveda contains about 6000 verses.</skos:note>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">Atharvaveda verse</rdfs:label>
    <rdfs:label xml:lang="sa">Atharvaveda pada</rdfs:label>
    <rdfs:label xml:lang="de">Atharvaveda-Vers</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Atharvaveda">
    <rdfs:comment xml:lang="en">Fourth Veda, containing the knowledge of atharvāṇas, the procedures of beliefs and rituals addressing practical issues of daily life of the Vedic society in Hinduism, compiled about 1200 to 1000 BC.</rdfs:comment>
    <rdfs:label xml:lang="de">Atharvaveda</rdfs:label>
    <rdfs:label xml:lang="en">Atharvaveda</rdfs:label>
    <rdfs:label xml:lang="sa">Atharvaveda</rdfs:label>
    <skos:note xml:lang="en">The Atharvaveda contains 20 Kandas.</skos:note>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/indology#Veda"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Kanda">
    <rdfs:comment xml:lang="en">Kanda of the Atharvaveda.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/indology#Expression"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="en">Atharvaveda kanda</rdfs:label>
    <skos:note xml:lang="en">The Atharvaveda contains 20 Kandas.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/literature#VerseSongCollection"/>
    <rdfs:label xml:lang="de">Atharvaveda Kanda</rdfs:label>
    <rdfs:label xml:lang="sa">Atharvaveda kāṇḍa</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Strophe">
    <rdfs:label xml:lang="en">Atharvaveda strophe</rdfs:label>
    <rdfs:comment xml:lang="en">Group of verses or 2 half-strophes in the Atharvaveda.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text-expression#ExpressionPart"/>
    <rdfs:label xml:lang="de">Atharvaveda-Strophe</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/prosodic-structure#Strophe"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text-editing#Recension"/>
    <rdfs:label xml:lang="de">Paippalada-Samhita</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:comment xml:lang="en">One of the nine recensions of the Atharvaveda, along the different shakhas or Hindu theological schools, specialized in learning Vedic texts. Only this one and the Shaunakiya recension are extent.</rdfs:comment>
    <rdfs:label xml:lang="en">Paippalada-Samhita</rdfs:label>
    <rdfs:label xml:lang="sa">Paippalāda-Samhitā</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/atharvaveda#Atharvaveda"/>
  </rdfs:Class>
</rdf:RDF>
