  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasDeathDay">
    <rdfs:comment xml:lang="en">Relating a person to the death day of that person, as an event.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">has death day</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:label xml:lang="de">hat Totestag</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasTitle">
    <rdfs:comment xml:lang="en">Relating a person to a title of that person.</rdfs:comment>
    <rdfs:label xml:lang="de">hat Titel</rdfs:label>
    <rdfs:label xml:lang="en">has title</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasName"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasMaxLifeSpan">
    <rdfs:label xml:lang="en">has maximum life span</rdfs:label>
    <rdfs:label xml:lang="de">hat maximale Lebensdauer</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://www.e-editiones.ch/ontology/human#hasLife"/>
      <rdf:Description rdf:about="http://www.e-editiones.ch/ontology/time#hasMaxDuration"/>
    </owl:propertyChainAxiom>
    <rdfs:comment xml:lang="en">Relating a person to a maximum life span the person has.</rdfs:comment>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Scientist">
    <rdfs:label xml:lang="en">scientist</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:label xml:lang="de">WissenschaftlerIn</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:comment xml:lang="en">Role of a person practicing science.</rdfs:comment>
    <rdfs:label xml:lang="fr">scientifique</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasDeathYearMonthPeriod">
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <rdfs:label xml:lang="de">hat Todesjahr-monatperiode</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:label xml:lang="en">has death year-month period</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a person to the person's death year-month period.</rdfs:comment>
    <skos:note xml:lang="en">Through a series of N3-rules a death year-month literal is converted to its year-month period with the respective calendar start and end date typed literal, and with the start and end Julian Day Number, to enable interval calculus for temporal reasoning.</skos:note>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasBiographyLiteral">
    <rdfs:label xml:lang="de">hat Biografieliterale</rdfs:label>
