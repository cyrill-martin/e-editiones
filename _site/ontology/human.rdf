>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasAlias">
    <rdfs:label xml:lang="en">has alias</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="de">hat Alias</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a person to an alias of that person.</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasName">
        <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasName"/>
        <rdfs:label xml:lang="en">has name</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="de">hat Name</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
        <rdfs:comment xml:lang="en">Relating a person to a name of that person.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
      </owl:DatatypeProperty>
    </rdfs:subPropertyOf>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasAlias"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasOccupationalRole">
    <rdfs:comment xml:lang="en">Relating a person to a current occupational role the person has.</rdfs:comment>
    <rdfs:label xml:lang="de">hat Tätigkeitsrolle</rdfs:label>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <rdfs:label xml:lang="en">has occupational role</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human#OccupationalRole"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#personHasRole">
        <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
        <rdfs:label xml:lang="en">person has role</rdfs:label>
        <rdfs:comment xml:lang="en">Relating a person to a role the person has.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
        <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/human#agentHasRole"/>
        <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
        <rdfs:label xml:lang="de">Person hat Rolle</rdfs:label>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasOwner">
    <rdfs:label xml:lang="en">has owner</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:comment xml:lang="en">Relating something to a human agent - a person or a group - owning it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range>
      <rdf:Description rdf:nodeID="ub124bL729C13">
        <rdfs:subClassOf>
          <owl:Restriction rdf:nodeID="ub124bL729C43">
            <owl:someValuesFrom rdf:resource="http://www.e-editiones.ch/ontology/human#AgentOwner"/>
            <owl:onProperty rdf:resource="http://www.e-editiones.ch/ontology/human#agentHasRole"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
      </rdf:Description>
    </rdfs:range>
    <rdfs:label xml:lang="de">hat EignerIn</rdfs:label>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/human">
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0"/>
    <dct:contributor xml:lang="en">Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland</dct:contributor>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:description xml:lang="en">Formal description of a human, general subclasses, related classes and properties.</dct:description>
    <dct:title xml:lang="en">An ontology about human</dct:title>
  </owl:Ontology>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasBirthdate">
    <rdfs:label xml:lang="de">hat Geburtsdatum</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:comment xml:lang="en">Relating a person to the birthdate of that person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <skos:note xml:lang="en">It is possible to find different birthdates in different historical sources; an overall certainty period can be calculated.</skos:note>
    <rdfs:label xml:lang="en">has birthdate</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Salon">
    <rdfs:label xml:lang="en">salon</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:comment xml:lang="en">Sociocultural gathering under the roof of an inspiring host.</rdfs:comment>
    <rdfs:label xml:lang="de">literarischer Salon</rdfs:label>
    <skos:note xml:lang="en">Exists from the seventeenth century AD until today.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#CulturalAction"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Vulgarizer">
    <rdfs:label xml:lang="fr">vulgarisateur</rdfs:label>
    <rdfs:label xml:lang="en">vulgarizer</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person making something, e.g. scientific text, less subtle, complex, or refined.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="de">Vulgarisator</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasPreserver">
    <rdfs:comment xml:lang="en">Relating a material resoruce to a human agent - a person or a group - preserving it.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:range>
      <rdf:Description rdf:nodeID="ub124bL745C13">
        <rdfs:subClassOf>
          <owl:Restriction rdf:nodeID="ub124bL745C43">
            <owl:someValuesFrom rdf:resource="http://www.e-editiones.ch/ontology/human#AgentPreserver"/>
            <owl:onProperty rdf:resource="http://www.e-editiones.ch/ontology/human#agentHasRole"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
      </rdf:Description>
    </rdfs:range>
    <rdfs:label xml:lang="de">hat Erhalter</rdfs:label>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#Matter"/>
    <rdfs:label xml:lang="en">has preserver</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Mathematician">
    <rdfs:label xml:lang="de">Mathematiker</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:label xml:lang="en">mathematician</rdfs:label>
