    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en">has user</rdfs:label>
    <rdfs:range>
      <rdf:Description rdf:nodeID="ub124bL720C13">
        <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
        <rdfs:subClassOf>
          <owl:Restriction rdf:nodeID="ub124bL720C43">
            <owl:someValuesFrom rdf:resource="http://www.e-editiones.ch/ontology/human#AgentUser"/>
            <owl:onProperty rdf:resource="http://www.e-editiones.ch/ontology/human#agentHasRole"/>
          </owl:Restriction>
        </rdfs:subClassOf>
      </rdf:Description>
    </rdfs:range>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasFamilyName">
