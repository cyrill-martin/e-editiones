    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">has biography literal</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a person to a biography of the person, as literal.</rdfs:comment>
