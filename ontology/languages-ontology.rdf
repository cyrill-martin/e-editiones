    <rdf:type rdf:resource="http://www.e-editiones.ch/ontology/concept#CodingSystem"/>
    <rdfs:label xml:lang="en">ISO 639</rdfs:label>
    <rdfs:comment xml:lang="en">International standard for codes for the representation of names of languages.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="https://www.iso.org/iso-639-language-codes.html"/>
  </rdfs:Class>

