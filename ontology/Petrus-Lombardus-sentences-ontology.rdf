  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/drcs">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology for the Digital Repertory of Commentaries on Peter Lombard’s Sentences</dc:title>
    <dc:description xml:lang="en">Formal description of specific concepts in the scientific study of Commentaries on Peter Lombard's Sentences, as in the Digital Repertory of Commentaries on Peter Lombard’s Sentences (DRCS).</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE, University of Basel, Switzerland</dc:creator>
    <dc:contributor xml:lang="en">Prof. Dr. Ueli Zahnd, Institut D'histoire De La Réformation, University of Geneva, Switzerland</dc:contributor>
    <dc:contributor xml:lang="en">Gabriel Müller, Faculty of Theology, University of Basel, Switzerland</dc:contributor>
    <rdfs:seeAlso rdf:resource="https://de.wikipedia.org/wiki/Friedrich_Stegmüller"/>
  </owl:Ontology>

