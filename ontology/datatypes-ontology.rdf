<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/datatypes">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology about datatypes</dc:title>
    <dc:description xml:lang="en">Formal description of datatypes, functioning as link between terminology and coding systems and OWL ontologies.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and ZÃ¼rich, Switzerland</dc:publisher>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/datatypes#gndScheme">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/datatypes#"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">GND-scheme</rdfs:label>
    <rdfs:comment xml:lang="en">The Integrated Authority File (in German Gemeinsame Normdatei, GND) is an authority file for persons, corporate bodies, conferences and events, geographic information, topics and works.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="https://www.dnb.de/EN/Standardisierung/GND/gnd_node.html"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/datatypes#gnd">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/datatypes#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <rdfs:label xml:lang="en">GND-datatype</rdfs:label>
    <rdfs:comment xml:lang="en">Datatype to type a GND-identifier.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/datatypes#hasGNDIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/datatypes#"/>
    <rdfs:label xml:lang="en">has GND-identifier</rdfs:label>
    <rdfs:label xml:lang="de">hat GND-Identifikator</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a resource to its GND-identifier.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/datatypes#hasOAIIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/datatypes#"/>
    <rdfs:label xml:lang="en">has OAI-identifier</rdfs:label>
    <rdfs:label xml:lang="de">hat OAI-Identifikator</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a resource to its OAI-code.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>

</rdf:RDF>