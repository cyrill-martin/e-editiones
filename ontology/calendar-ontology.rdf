  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/calendar#hasProlepticJulianEndDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/calendar#"/>
