  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/calendar#hasGregorianEndDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/calendar#"/>
    <rdfs:label xml:lang="en">has Gregorian end date</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an event to its Gregorian end date.</rdfs:comment>
