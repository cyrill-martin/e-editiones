  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/calendar#hasGregorianStartDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/calendar#"/>
    <rdfs:label xml:lang="en">has Gregorian start date</rdfs:label>
