<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/time">
    <dc:title xml:lang="en">An ontology about time relations</dc:title>
    <dc:description xml:lang="en">Formal description of time relations.</dc:description>
    <skos:note xml:lang="en">Basic model is a space-time thing, which can be described in different levels of abstraction, mostly with input and output, with a beginning and end, wherein a thing can change and/or act, in a certain way.</skos:note>
    <skos:note xml:lang="en">For formal temporal reasoning, using e.g. the Unix Epoch time.</skos:note>
    <skos:note xml:lang="en">There are properties with list as range. A list keeps things together in a better way than a blank node and improves reasoning performance. List content is tripled when needed, e.g. to permit SPARQL querying.</skos:note>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and ZÃ¼rich, Switzerland</dc:publisher>
    <rdfs:seeAlso rdf:resource="http://eulersharp.sourceforge.net/2007/07test/rifP.n3"/>
    <rdfs:seeAlso rdf:resource="http://eulersharp.sourceforge.net/2003/03swap/prolog"/>
    <rdfs:seeAlso rdf:resource="http://eulersharp.sourceforge.net/2007/07test/biP.n3"/>
    <rdfs:seeAlso rdf:resource="http://eulersharp.sourceforge.net/2003/03swap/eventTime_rules"/>
  </owl:Ontology>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasStartDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its start date time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasEarliestStartDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its N3-rule-calculated earliest possible start date time, based on a set maximum event duration, in absence of the start date time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasStartDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its start date.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasEndDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its end date time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasLatestEndDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its N3-rule-calculated latest possible end date time, based on a set maximum event duration, in absence of the end date time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasEndDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its end date.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Specifying a point in time of something with a date time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasDate">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has date</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its date.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasOutputDateTime">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has output date-time</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a process to its output date-time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Process"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasYearLiteral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has year literal</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its year represented by a literal.</rdfs:comment>
    <skos:note xml:lang="en">Used in the premise of an N3-rule to convert a year literal to a period of that year in the respective calendar with the start and end date represented by a list of 3 literals.</skos:note>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasYearMonthLiteralList">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has year-month literal list</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its year-month represented by a list of 2 literals.</rdfs:comment>
    <skos:note xml:lang="en">Used in the premise of an N3-rule to convert a year-month to a period of that month in that year in the respective calendar with the start and end date represented by a list of 3 literals.</skos:note>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasDateLiteralList">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has date literal list</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its date represented by a list of 3 literals.</rdfs:comment>
    <skos:note xml:lang="en">Used in the premise of an N3-rule to convert a date literal list to the respective calendar date represented by a list of 3 literals.</skos:note>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasYearPeriod">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has year period</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its year period.</rdfs:comment>
    <skos:note xml:lang="en">Through a series of N3-rules a year literal is converted to its year period with the respective calendar start and end date typed literal, and with the start and end Julian Day Number, to enable interval calculus for temporal reasoning.</skos:note>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasYearMonthPeriod">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has year-month period</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its year-month period.</rdfs:comment>
    <skos:note xml:lang="en">Through a series of N3-rules a year-month literal is converted to its year-month period with the respective calendar start and end date typed literal, and with the start and end Julian Day Number, to enable interval calculus for temporal reasoning.</skos:note>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasDuration">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Specifying a duration of an event.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasMaxDuration">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">has maximum duration</rdfs:label>
    <rdfs:label xml:lang="de">hat maximale Dauer</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an event to its maximum duration.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasDurationDescription">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Specifying a duration description for a temporal entity of something.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/time#calculatingDuration">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating an event to its calculated duration.</rdfs:comment>
    <rdfs:comment xml:lang="en">Such a triple invokes in an N3 rule a backward N3 rule calculating an event's duration in premise and returning the duration.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#timeRelatedTo">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Relating 2 events in time.</rdfs:comment>
    <owl:oneOf>
      <rdf:Description>
        <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#beginsBefore"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first>
              <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#beginsBeforeEndOf">
                <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                <rdfs:comment xml:lang="en">Linking a beginning event to a later ending of another event.</rdfs:comment>
                <skos:note xml:lang="en">Used in an N3 rule conclusion.</skos:note>
                <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                <owl:propertyChainAxiom>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#beginsDuring"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#endsBefore"/>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </rdf:Description>
                    </rdf:rest>

                  </rdf:Description>
                </owl:propertyChainAxiom>

                <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
              </owl:ObjectProperty>
            </rdf:first>

            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <owl:SymmetricProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasSameBeginningAs">
                    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                    <rdfs:comment xml:lang="en">Linking 2 events beginning on the same moment.</rdfs:comment>
                    <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                  </owl:SymmetricProperty>
                </rdf:first>

                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#beginsDuring"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#beginsAfter"/>
                        <rdf:rest>
                          <rdf:Description>
                            <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#endsBefore"/>
                            <rdf:rest>
                              <rdf:Description>
                                <rdf:first>
                                  <owl:SymmetricProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasSameEndAs">
                                    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                    <rdfs:comment xml:lang="en">Linking 2 events ending on the same moment.</rdfs:comment>
                                    <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                  </owl:SymmetricProperty>
                                </rdf:first>

                                <rdf:rest>
                                  <rdf:Description>
                                    <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#endsDuring"/>
                                    <rdf:rest>
                                      <rdf:Description>
                                        <rdf:first>
                                          <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#endsAfter">
                                            <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
                                            <rdfs:comment xml:lang="en">Linking a later ending to an earlier ending event.</rdfs:comment>
                                            <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                            <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                            <owl:inverseOf rdf:resource="http://www.e-editiones.ch/ontology/time#endsBefore"/>
                                            <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                            <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                          </owl:ObjectProperty>
                                        </rdf:first>

                                        <rdf:rest>
                                          <rdf:Description>
                                            <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#before"/>
                                            <rdf:rest>
                                              <rdf:Description>
                                                <rdf:first>
                                                  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#after">
                                                    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                                    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
                                                    <rdfs:comment xml:lang="en">Linking a later to an earlier event.</rdfs:comment>
                                                    <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                                    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                                    <owl:inverseOf rdf:resource="http://www.e-editiones.ch/ontology/time#before"/>
                                                    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                  </owl:ObjectProperty>
                                                </rdf:first>

                                                <rdf:rest>
                                                  <rdf:Description>
                                                    <rdf:first>
                                                      <owl:SymmetricProperty rdf:about="http://www.e-editiones.ch/ontology/time#overlaps">
                                                        <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                                        <rdfs:comment xml:lang="en">Specifying an event overlapping another event.</rdfs:comment>
                                                        <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                                        <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                                        <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                        <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                      </owl:SymmetricProperty>
                                                    </rdf:first>

                                                    <rdf:rest>
                                                      <rdf:Description>
                                                        <rdf:first>
                                                          <owl:SymmetricProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasSameTimeAs">
                                                            <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                                            <rdfs:comment xml:lang="en">Linking 2 events happening at the same time.</rdfs:comment>
                                                            <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                                            <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                                            <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                            <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                          </owl:SymmetricProperty>
                                                        </rdf:first>

                                                        <rdf:rest>
                                                          <rdf:Description>
                                                            <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#during"/>
                                                            <rdf:rest>
                                                              <rdf:Description>
                                                                <rdf:first>
                                                                  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#includes">
                                                                    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                                                    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
                                                                    <rdfs:comment xml:lang="en">Specifying an event happening during another event.</rdfs:comment>
                                                                    <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                                                    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                                                    <owl:inverseOf rdf:resource="http://www.e-editiones.ch/ontology/time#during"/>
                                                                    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                                    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                                  </owl:ObjectProperty>
                                                                </rdf:first>

                                                                <rdf:rest>
                                                                  <rdf:Description>
                                                                    <rdf:first>
                                                                      <owl:SymmetricProperty rdf:about="http://www.e-editiones.ch/ontology/time#hasSameDurationAs">
                                                                        <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/time#"/>
                                                                        <rdfs:comment xml:lang="en">Linking 2 events having the same duration.</rdfs:comment>
                                                                        <skos:note xml:lang="en">Further formalized in an N3 rule by calculation in http://www.e-editiones.ch/ontology/eventTime_rules.n3, enabling reasoning with date/time values.</skos:note>
                                                                        <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/time#timeRelatedTo"/>
                                                                        <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                                        <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/event#Event"/>
                                                                      </owl:SymmetricProperty>
                                                                    </rdf:first>

                                                                    <rdf:rest>
                                                                      <rdf:Description>
                                                                        <rdf:first rdf:resource="http://www.e-editiones.ch/ontology/time#takesLongerThan"/>
                                                                        <rdf:rest>
                                                                          <rdf:Description>
                                                                            <rdf:first>
                                                                              <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/time#takesLessLongThan">
