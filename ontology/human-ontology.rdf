<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:ns0="http://purl.uniprot.org/core/">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/human">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology about human</dc:title>
    <dc:description xml:lang="en">Formal description of a human, general subclasses, related classes and properties.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:contributor xml:lang="en">Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland</dc:contributor>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dc:publisher>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Agent">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human agent</rdfs:label>
    <rdfs:label xml:lang="de">menschliche Agent</rdfs:label>
    <rdfs:comment xml:lang="en">Agent being one or more humans</rdfs:comment>
    <rdfs:comment xml:lang="de">Agent einer oder mehrerer Menschen betreffend.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/agent#Agent"/>
    <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Human">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human</rdfs:label>
    <rdfs:label xml:lang="de">Mensch</rdfs:label>
    <rdfs:label xml:lang="fr">homme</rdfs:label>
    <rdfs:label xml:lang="it">huomo</rdfs:label>
    <rdfs:comment xml:lang="en">Member of the subspecies Homo sapiens sapiens.</rdfs:comment>
    <skos:note xml:lang="en">A human lives but is not necessarily born, i.e. a human embryo or foetus.</skos:note>
    <ns0:scientificName xml:lang="en">Homo sapiens sapiens</ns0:scientificName>
    <ns0:rank rdf:resource="http://purl.uniprot.org/core/Subspecies"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
    <rdfs:subClassOf rdf:resource="http://purl.uniprot.org/taxonomy/9606"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human#hasBiologicalSex">
            <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
            <rdfs:label xml:lang="en">has biological sex</rdfs:label>
            <rdfs:label xml:lang="de">hat biologisches Geschlecht</rdfs:label>
            <rdfs:comment xml:lang="en">Relating a human to a biological sex of that human.</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Human"/>
            <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human#BiologicalSex"/>
          </owl:ObjectProperty>
        </owl:onProperty>

        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Person">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person</rdfs:label>
    <rdfs:label xml:lang="de">Person</rdfs:label>
    <rdfs:label xml:lang="fr">personne</rdfs:label>
    <rdfs:label xml:lang="it">persona</rdfs:label>
    <rdfs:comment xml:lang="en">A real born living human.</rdfs:comment>
    <skos:note xml:lang="en">A foaf:Person class member is born, but can be alive or dead, real or imaginary. In this more restricted meaning a person can do things during life and can be talked about after dead, having done things during life. An imaginary person is a thing that can have every conceivable property.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Human"/>
    <rdfs:subClassOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F10_Person"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#UnidentifiedPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">unidentified person</rdfs:label>
    <rdfs:label xml:lang="de">unidentifiziert Person</rdfs:label>
    <rdfs:label xml:lang="fr">personne non identifiée</rdfs:label>
    <rdfs:label xml:lang="it">persona non identificata</rdfs:label>
    <rdfs:comment xml:lang="en">Person who's identity is unknown.</rdfs:comment>
    <skos:note xml:lang="en">One can state about a book that the person who wrote it as author is unidentified. The person exists or has existed, thus the resource can be represented by an IRI for *a* person (not necessarily a blank node (anonymous resource)), but the information to identify the person is missing. This knowledge of absence of certain information is made explicit. In other words, there is a distinction between the IRI identifying *a* person, and the information, provided by properties, e.g. birth date, family name, christian name, permitting to sufficiently describe thé person to be identified.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonLike">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person-like resource</rdfs:label>
    <rdfs:label xml:lang="de">Person-ähnliche Entität</rdfs:label>
    <rdfs:comment xml:lang="en">Person like resource having non-human features.</rdfs:comment>
    <skos:note xml:lang="en">E.g. Jesus is for a believer a person also having non-human features.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Group">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">group of humans</rdfs:label>
    <rdfs:label xml:lang="de">Menschengruppe</rdfs:label>
    <rdfs:comment xml:lang="en">Group of humans.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/agent#Group"/>
    <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Action">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human action</rdfs:label>
    <rdfs:label xml:lang="de">menschliche Aktion</rdfs:label>
    <rdfs:comment xml:lang="en">Action involving a human agent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/event#Action"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Procedure">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human procedure</rdfs:label>
    <rdfs:label xml:lang="de">menschliches Verfahren</rdfs:label>
    <rdfs:comment xml:lang="en">Procedure with a human agent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/event#Procedure"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#BiologicalSex">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human biological sex</rdfs:label>
    <rdfs:label xml:lang="de">menschliches biologisches Geschlecht</rdfs:label>
    <rdfs:comment xml:lang="en">Biological trait of a human based on reproductive functions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <owl:disjointUnionOf>
      <rdf:Description>
        <rdf:first>
          <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Female">
            <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
            <rdfs:label xml:lang="en">human female</rdfs:label>
            <rdfs:label xml:lang="de">menschlich weiblich</rdfs:label>
            <rdfs:comment xml:lang="en">Being of a human female sex.</rdfs:comment>
            <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#BiologicalSex"/>
          </rdfs:Class>
        </rdf:first>

        <rdf:rest>
          <rdf:Description>
            <rdf:first>
              <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Male">
                <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
                <rdfs:label xml:lang="en">human male</rdfs:label>
                <rdfs:label xml:lang="de">menschlich männlich</rdfs:label>
                <rdfs:comment xml:lang="en">Being of a human male sex.</rdfs:comment>
                <skos:note xml:lang="en">In adulthood producing spermatozoa for fertilizing female ova.</skos:note>
                <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#BiologicalSex"/>
              </rdfs:Class>
            </rdf:first>

            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Intersexual">
                    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
                    <rdfs:label xml:lang="en">human intersexual</rdfs:label>
                    <rdfs:label xml:lang="de">menschlich intersexual</rdfs:label>
                    <rdfs:comment xml:lang="en">Being of a sex that is neither male nor female, with biological characteristics of both.</rdfs:comment>
                    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#BiologicalSex"/>
                  </rdfs:Class>
                </rdf:first>

                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </rdf:rest>

      </rdf:Description>
    </owl:disjointUnionOf>

  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Ability">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human ability</rdfs:label>
    <rdfs:label xml:lang="de">menschliche Fähigkeit</rdfs:label>
    <rdfs:comment xml:lang="en">Possibility of a person to do something.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentRole">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human agent role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle eines menschlichen Agenten</rdfs:label>
    <rdfs:comment xml:lang="en">Role a human agent has as a person or group.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/agent#Role"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonRole">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle einer Person</rdfs:label>
    <rdfs:comment xml:lang="en">Role a person has as a social entity.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#GroupRole">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">human group role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle einer menschlichen Gruppe</rdfs:label>
    <rdfs:comment xml:lang="en">Role a group of persons has.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonLife">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person's life</rdfs:label>
    <rdfs:label xml:lang="de">Leben einer Person</rdfs:label>
    <rdfs:comment xml:lang="en">Life of a human from birth until death.</rdfs:comment>
    <skos:note xml:lang="en">Considered as an action wherein a person is an agent.</skos:note>
    <skos:note xml:lang="en">No satisfying biological description of onset of a human life; set at birth when a human becomes a person.</skos:note>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Action"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/event#Existence"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Use">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">use</rdfs:label>
    <rdfs:label xml:lang="de">Verwendungszweck</rdfs:label>
    <rdfs:comment xml:lang="en">Purpose for or way in which something is used.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentUser">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent user</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, person or group, taking something into service or applying for a purpose.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/agent#User"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentGiver">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent giver</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Geber</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, person or group, transferring the possession of something to another agent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentOwner">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent owner</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Besitzer</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, a person or a group, having a property.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonOwner">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person owner</rdfs:label>
    <rdfs:label xml:lang="de">Person-BesitzerIn</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person having a property.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentOwner"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Property">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">property</rdfs:label>
    <rdfs:label xml:lang="de">Besitz</rdfs:label>
    <rdfs:comment xml:lang="en">Something physical or non-physical belonging to someone.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentSponsor">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent sponsor</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Sponsor</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, a person or a group, giving material or financial support for an action.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonSponsor">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person sponsor</rdfs:label>
    <rdfs:label xml:lang="de">Person-SponsorIn</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person as a sponsor.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentSponsor"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentPreserver">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent preserver</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Erhalter</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, a person or a group, maintaining something material.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentSender">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent sender</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Sender</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, a person or a group, sending something physical.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonSender">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person sender</rdfs:label>
    <rdfs:label xml:lang="de">Person-Sender</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person sending something physical.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentSender"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#AgentReceiver">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">agent receiver</rdfs:label>
    <rdfs:label xml:lang="de">Agent-Empfänger</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a human agent, a person or a group, receiving something physical.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#PersonReceiver">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">person receiver</rdfs:label>
    <rdfs:label xml:lang="de">Person-Empfänger</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person receiving something physical.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#AgentReceiver"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#CulturalAction">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">cultural action</rdfs:label>
    <rdfs:label xml:lang="de">kulturelle Aktion</rdfs:label>
    <rdfs:comment xml:lang="en">Action having a cultural purpose.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Action"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#Occupation">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">occupation</rdfs:label>
    <rdfs:label xml:lang="de">Beruf</rdfs:label>
    <rdfs:comment xml:lang="en">Action serving as a person's regular source of livelihood.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#Action"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human#OccupationalRole">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human#"/>
    <rdfs:label xml:lang="en">occupational role</rdfs:label>
    <rdfs:label xml:lang="de">Berufsrolle</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person in an occupation.</rdfs:comment>
