<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/dietrich">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology about father Joseph Dietrich's monastery diary</dc:title>
    <dc:description xml:lang="en">Formal description of specific concepts in the scientific study &quot;Das Kloster-Tagebuch des Einsiedler Paters Joseph Dietrich, 1670-1704&quot;.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:contributor xml:lang="en">Reinhard Priber, tool coordinator and software developer, University of Bern, Switzerland</dc:contributor>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and ZÃ¼rich, Switzerland</dc:publisher>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/dietrich#DiaryEntry">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/dietrich#"/>
    <rdfs:label xml:lang="en">diary entry by father Joseph Dietrich</rdfs:label>
    <rdfs:label xml:lang="de">Tagebucheintrag des Paters Joseph Dietrich</rdfs:label>
    <rdfs:comment xml:lang="en">Diary entry authored by father Joseph Dietrich.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text#HandwrittenText"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text-expression#DiaryEntry"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/dietrich#DiaryVolume">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/dietrich#"/>
    <rdfs:label xml:lang="en">diary volume by father Joseph Dietrich</rdfs:label>
    <rdfs:label xml:lang="de">Tagebuch-Band des Paters Joseph Dietrich</rdfs:label>
    <rdfs:comment xml:lang="en">Diary volume written and authored by father Joseph Dietrich.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#Manuscript"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#Diary"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#Volume"/>
  </rdfs:Class>

</rdf:RDF>
