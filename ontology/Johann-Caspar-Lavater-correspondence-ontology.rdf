<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/lavater">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology about Johann Caspar Lavater's correspondence</dc:title>
    <dc:description xml:lang="en">Formal description of specific concepts in the critical edition of correspondence of Johann Caspar Lavater, 15 November 1741 - 2 January 1801.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:contributor xml:lang="en">Reto Baumgartner, tool coordinator and software developer, University of Zürich, Switzerland</dc:contributor>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dc:publisher>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/lavater#LetterExpression">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/lavater#"/>
    <rdfs:label xml:lang="en">Johann Caspar Lavater letter expression</rdfs:label>
    <rdfs:label xml:lang="de">Brief-Expression Johann Caspar Lavaters</rdfs:label>
    <rdfs:comment xml:lang="en">Letter expression either authored by or written to by Johann Caspar Lavater.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/letter-corresponding#Expression"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/text#HandwrittenText"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/lavater#Letter">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/lavater#"/>
    <rdfs:label xml:lang="en">Johann Caspar Lavater letter</rdfs:label>
    <rdfs:label xml:lang="de">Johann Caspar Lavater-Brief</rdfs:label>
    <rdfs:label xml:lang="fr">lettre de Johann Caspar Lavater</rdfs:label>
    <rdfs:label xml:lang="it">lettera di Johann Caspar Lavater</rdfs:label>
    <rdfs:comment xml:lang="en">Letter send by or to Johann Caspar Lavater.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#ConvoluteElement"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#ArchiveElement"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/lavater#Page">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/lavater#"/>
    <rdfs:label xml:lang="en">Johann Caspar Lavater letter page</rdfs:label>
    <rdfs:label xml:lang="de">Seite eines Briefes Johann Caspar Lavaters</rdfs:label>
    <rdfs:comment xml:lang="en">Page of a letter of Johann Caspar Lavater.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#Page"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/lavater#LetterConvolute">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/lavater#"/>
    <rdfs:label xml:lang="en">Johann Caspar Lavater letter convolute</rdfs:label>
    <rdfs:label xml:lang="de">Briefekonvolut Johann Caspar Lavaters</rdfs:label>
    <rdfs:comment xml:lang="en">Convolute of letters send by and to Johann Caspar Lavater.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#Convolute"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#ArchiveElement"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/lavater#EditedLetter">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/lavater#"/>
    <rdfs:label xml:lang="en">edited Johann Caspar Lavater letter expression</rdfs:label>
    <rdfs:label xml:lang="de">edierter Brief-Expression Johann Caspar Lavaters</rdfs:label>
    <rdfs:comment xml:lang="en">Letter expression either authored or received by Johann Caspar Lavater, and critically edited.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/scholarly-editing#CriticalEdition"/>
  </rdfs:Class>

</rdf:RDF>
